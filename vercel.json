{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "crons": [
    {
      "path": "/sendNotification",
      "schedule": "0 0 * * *"
    }
  ]
  }